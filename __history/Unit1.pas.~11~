unit Unit1;

interface
  type
    User  = class
        private _id :Integer;
        public
          userName :String;
          password :String ;

        public constructor Create(id :Integer ; username :String ;pass :String);
    end;

implementation
     constructor User.Create(id :Integer ; username :String ; pass :String);
     begin
         self._id   := id;
         self.userName :=  username;
         self.password := pass;
     end;
end.
